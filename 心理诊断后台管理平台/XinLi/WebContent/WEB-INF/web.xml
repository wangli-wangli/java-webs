<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>XinLi</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>CharFilter</filter-name>
    <filter-class>filter.CharFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>patientController</servlet-name>
    <servlet-class>servlet.patientController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>patientController</servlet-name>
    <url-pattern>/Dangan-add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>patientController</servlet-name>
    <url-pattern>/visitor/Dangan-add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>patientController</servlet-name>
    <url-pattern>/Dangan-edit.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>patientController</servlet-name>
    <url-pattern>/Dangan-delete.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>webController</servlet-name>
    <servlet-class>servlet.webController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>webController</servlet-name>
    <url-pattern>/home-add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>webController</servlet-name>
    <url-pattern>/home-delete.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>webController</servlet-name>
    <url-pattern>/home-delete2.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>webController</servlet-name>
    <url-pattern>/home-edit.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>resultController</servlet-name>
    <servlet-class>servlet.resultController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>resultController</servlet-name>
    <url-pattern>/Data-management-delete.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>resultController</servlet-name>
    <url-pattern>/Data-management-daochu.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userController</servlet-name>
    <servlet-class>servlet.userController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userController</servlet-name>
    <url-pattern>/user-management-add.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>userController</servlet-name>
    <url-pattern>/user-management-delete2.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>userController</servlet-name>
    <url-pattern>/user-management-delete.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>userController</servlet-name>
    <url-pattern>/user-password.do</url-pattern>
  </servlet-mapping>
</web-app>